<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.sc2geeks.replay.model.MapEntity" table="rep_map" schema="" catalog="sc2geeks">
		<id name="mapId" column="MapId">
			<generator class="identity"/>
		</id>
		<property name="name" column="Name"/>
		<property name="bnetUrl" column="BnetUrl"/>
		<property name="imageFile" column="ImageFile"/>
		<property name="width" column="Width"/>
		<property name="height" column="Height"/>
		<property name="lastEditDate" column="LastEditDate"></property>
	</class>
</hibernate-mapping>